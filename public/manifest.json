{
  "name": "minimap",
  "version": "0.1",
  "manifest_version": 3,
  "permissions": [
    "browsingData",
    "storage",
    "windows",
    "tabs",
    "activeTab",
    "sidePanel"
  ],
  "action": {
    "default_title": "Click to open panel"
  },
  "background": {
    "service_worker": "./service-worker.js"
  },
  "chrome_url_overrides": {
    "newtab": "./index.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "linux": "Alt+D",
        "mac": "MacCtrl+D",
        "windows": "Alt+D"
      }
    }
  },
  "side_panel": {
    "default_path": "./index.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
