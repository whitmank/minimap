{
    "manifest_version": 3,
    "name": "Mini Map - DEV",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "browsingData",
        "windows"
    ],
    "chrome_url_overrides": {
        "bookmarks": "/src/chrome/map.html"
    },
    "background": {
        "service_worker": "./src/chrome/background.js"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    }
}
